{
  "version": 2,
  "builds": [
    {
      "src": "src/bot-working.js",
      "use": "@vercel/node"
    },
    {
      "src": "web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/bot-working.js"
    },
    {
      "src": "/(.*)",
      "dest": "web/$1"
    }
  ],
  "env": {
    "BOT_TOKEN": "@bot_token"
  }
}